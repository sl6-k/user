/* pages/me/integralSettlement/integralSettlement.wxss */
/* 底部 */

.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  border-top: 1px solid #eee;
  padding: 0 16px;
  height: 50px;
  box-sizing: border-box;
}

.footer .btn {
  width: 30%;
  height: 36px;
  border-radius: 18px;
  line-height: 36px;
  color: #fff;
  text-align: center;
  font-size: 14px;
  background: #f15a24;
}

.footer .price {
  font-weight: 600;
  color: #f15a24;
  font-size: 18px;
}

.footer .price text:first-child {
  color: #888;
  font-size: 14px;
  font-weight: normal;
  margin-right: 8px;
}

/* .footer .price text:last-child {
  color: #888;
  font-size: 14px;
  font-weight: normal;
  text-decoration: line-through;
  margin-left: 8px;
} */

/* 内容 */

.content {
  padding-bottom: 60px;
}

.content .site {
  background: #fff;
  margin-bottom: 15px;
}

.content .site .userBox {
  flex: 1;
  padding-right: 15px;
  box-sizing: border-box;
}

.content .site .userText {
  border-bottom: 1px solid #eee;
  height: 50px;
  padding: 0 16px;
}

.content .site .userText .name {
  flex: 1;
  display: flex;
  font-size: 14px;
  color: #444;
  /* align-items: right; */
}

.content .site .userText .icon {
  width: 18px;
  height: 18px;
  margin-right: 10px;
}

.content .site .userText .rightIcon {
  width: 14px;
  height: 14px;
}

.content .site .shopLocaltion {
  padding: 0 16px;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.content .site .shopLocaltion .timeBox {
  color: #444;
  font-size: 16px;
  height: 40px;
  justify-content: flex-start;
}

.content .site .shopLocaltion  .icon {
  width: 12px;
  height: 12px;
  margin-right: 5px;
  margin-left: 3px;
}

.content .site .shopLocaltion .timeText {
  flex: 1;
  font-size: 14px;
  color: #888;
}

.content .site .shopLocaltion .localBox {
  padding-top: 6px;
  padding-bottom: 10px;
}

.content .site .shopLocaltion .localBox .shopSite {
  flex: 1;
}

.content .site .shopLocaltion .localBox .shopSite .localIcon {
  width: 14px;
  height: 14px;
  margin-right: 6px;
}

.shopLocaltion .localBox  .km {
  color: #444;
  font-size: 14px;
  margin-left: 12px;
}

.shopLocaltion .rightIcon {
  width: 14px;
  /* height: 14px; */
  margin-left: 8px;
  display: flex;
  align-items: center;
}

.shopLocaltion .rightIcon .images {
  width: 100%;
  height: 100%;
}

.shopLocaltion .localBox  .shopName {
  flex: 1;
  font-size: 14px;
  word-break: break-all;
}

.content .shoppListBox {
  background: #fff;
  margin-bottom: 15px;
}

.content .shoppListBox  .shopList:last-child {
  border: none;
}

.shopList {
  background: #fff;
  padding: 10px 16px;
  border-bottom: 1px solid #eee;
}

.content .shopList .shopImg {
  width: 80px;
  height: 80px;
  margin-right: 10px;
}

.content .shopList .shopText {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.content .shopList .shopText .title {
  font-size: 14px;
  font-weight: 500;
  word-break: break-all;
}

.content .shopList .shopText .guige {
  font-size: 12px;
  font-weight: 500;
  word-break: break-all;
  color: #666;
}

.content .shopList .shopText .number {
  font-size: 12px;
  font-weight: 500;
  word-break: break-all;
  color: #faaf3b;
}

.content .shopList .priceQuantity {
  padding-top: 10px;
}

.content .shopList .priceQuantity .price {
  font-size: 16px;
  color: #333;
}

.content .shopList .priceQuantity .price text:last-child {
  font-size: 16px;
  color: #faaf3b;
}

.content .shopList .buyCount {
  margin-top: 10px;
  display: flex;
  flex: 1;
  /* flex-wrap: nowrap;
  justify-content: space-between; */
}

.content .shopList .count {
  font-size: 12px;
  color: #333;
  margin-left: 15px;
}

.content .blockBox {
  background: #fff;
  margin-bottom: 15px;
}

.content .blockBox .blockLists {
  height: 50px;
  padding: 0 16px;
  border-bottom: 1px solid #eee;
}

.content .blockBox .blockLists:last-child {
  border-bottom: none;
}

.content .blockBox .blockLists .title {
  flex: 1;
  font-size: 15px;
}

.content .blockBox .blockLists .icon {
  width: 12px;
  height: 12px;
  margin-left: 12px;
}

.content .countBox {
  background: #fff;
}

.content .countBox .countList {
  height: 40px;
  padding: 0 16px;
  font-size: 15px;
}

.content .countBox .countList text:last-child {
  font-size: 13px;
}

.content .countBox .countList.red text:last-child {
  color: red;
}

.content .countBox .countList.del text:last-child {
  color: #ccc;
  text-decoration: line-through;
}

.content .countBox .countList.green text:last-child {
  color: rgb(120, 190, 97);
}

.setupBox {
  background: #fff;
  height: 50px;
  padding: 0 16px;
  font-size: 15px;
  border: 1px solid #eee;
}

.setupBox .btnBlock {
  display: flex;
}

.setupBox .btnBlock .btn {
  margin-left: 12px;
  font-size: 13px;
  color: #ccc;
  border-radius: 16px;
  height: 32px;
  line-height: 30px;
  padding: 0 16px;
  border: 1px solid #eee;
}

.setupBox .btnBlock .btn.active {
  color: #faaf3b;
  border-color: #faaf3b;
}

/* 门店 */

.shopCainerter {
  border-top: 1px solid #eee;
}

.shopCainerter .shopList {
  padding: 10px 16px;
  border-bottom: 1px solid #eee;
  background: #fff;
  display: flex;
  justify-content: space-between;
}

.shopCainerter .shopList .shopPhoto {
  width: 90px;
  height: 90px;
}

.shopCainerter .shopList .shopPhoto image {
  display: block;
  width: 100%;
  height: 100%;
}

.shopCainerter .shopList .shopText {
  flex: 1;
  padding-left: 10px;
  box-sizing: border-box;
}

.shopCainerter .shopList .shopText .shopTitle {
  font-weight: 600;
  font-size: 20px;
}

.shopCainerter .shopList .shopText  .shopEvaluate {
  line-height: 34px;
  color: #888;
  font-size: 13px;
}

.shopCainerter .shopList .shopText  .shopEvaluate text:last-child {
  color: #faaf3b;
  font-size: 13px;
  margin-left: 8px;
}

.shopCainerter .shopList .shopText .shopLocaltion {
  overflow: hidden;
  color: #444;
  font-size: 13px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 8px;
}

.shopCainerter .shopList .shopText .shopLocaltion .km {
  margin-left: 12px;
  color: #dfdfdf;
}

.shopCainerter .shopList .shopText .shopLocaltion .localtion {
  overflow: hidden;
  width: 70%;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.shopCainerter .shopList .shopText .shopTag {
  overflow: hidden;
  display: flex;
  color: #ccc;
  font-size: 12px;
  margin-right: 6px;
  margin-bottom: 5px;
}

.shopCainerter .shopList .shopText .shopTag .tag {
  border: 1px solid #ccc;
  border-radius: 4px;
  padding: 1px 4px;
  display: block;
  font-size: 10px;
}

