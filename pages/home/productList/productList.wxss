.selectBox {
  /* position: fixed; */
  top: 0px;
  left: 0px;
  right: 0px;
  height: 40px;
  align-items: center;
  background: #fff;
  display: flex;
  justify-content: space-between;
  padding: 10px 0;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
}

.selectBox .selectList {
  display: flex;
  width: 50%;
  justify-content: center;
  align-items: center;
  font-size: 12px;
  border-right: 1px solid #eee;
}

.selectBox .selectList:last-child {
  border: none;
}

.selectBox .selectList .icon {
  width: 10px;
  height: 10px;
  margin-left: 8px;
}

.selectBox .selectList .isTop {
  width: 10px;
  height: 10px;
  line-height: 10px;
  color: #faaf3b;
  border: 1px solid #faaf3b;
}

.selectBox .selectList .top {
  width: 10px;
  height: 10px;
  line-height: 10px;
  color: #fff;
  border: 1px solid #ccc;
}

.selectBox .selectList .icon image {
  width: 100%;
  height: 100%;
  display: block;
}

.shopLIistBox {
  padding: 0px 16px;
  box-sizing: border-box;
}

.shopLIistBox .listBox {
  background: #fff;
  padding: 12px 16px;
  border-radius: 9px;
  overflow: hidden;
  display: flex;
  margin-bottom: 12px;
}

.shopLIistBox .listBox .shopImg {
  width: 90px;
  height: 90px;
  margin-right: 12px;
}

.shopLIistBox .listBox .content {
  flex: 1;
}

.shopLIistBox .listBox .content .title {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  font-size: 16px;
  color: #444;
  word-break: break-all;
  margin-bottom: 6px;
}

.shopLIistBox .listBox .content  .msg {
  font-size: 13px;
  color: #faaf3b;
  margin-bottom: 6px;
}

.shopLIistBox .listBox .size {
  font-size: 12px;
  color: #ccc;
  margin-bottom: 4px;
  padding: 5px 0;
}

.shopLIistBox .listBox .size .sign {
  border: 1px solid #ccc;
  margin-right: 10px;
  padding: 0px 5px;
  font-size: 10px;
  border-radius: 3px;
}

.shopLIistBox .listBox .grade {
  display: flex;
  font-size: 13px;
  color: #faaf3b;
  margin-bottom: 6px;
}

.numberBg {
  width: 110px;
  height: 14px;
  margin-right: 12px;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}

.numberBg image {
  width: 110px;
  height: 14px;
  display: block;
}

.numberBg .ovflow {
  width: 70%;
  overflow: hidden;
}

.shopLIistBox .listBox .price {
  font-weight: 600;
  text-align: right;
  font-size: 16px;
  color: #faaf3b;
}

.loadingMsg {
  text-align: center;
  font-size: 14px;
  color: #ccc;
  padding: 20px 0;
}

.layer {
  position: fixed;
  top: 0;
  left: 0px;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 99999;
}

.layer .layer-label {
  width: 30%;
  text-align: center;
  background: #f1f2f6;
  color: #1a1b1f;
  font-size: 12px;
  margin-bottom: 10px;
  padding-top: 3px;
  padding-bottom: 3px;
  border-radius: 5px;
}

.layer .flex-label {
  width: 45%;
  text-align: center;
  background: #f1f2f6;
  color: #1a1b1f;
  font-size: 12px;
  margin-bottom: 10px;
  padding-top: 5px;
  padding-bottom: 5px;
  border-radius: 5px;
}

.layer .flex {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.layer .flex .active {
  border: 1px solid #ffb12a;
  color: #ffb12a;
  background: none;
}

.layer .scrollBox {
  position: absolute;
  right: 0;
  border-top: 1px solid #eee;
  width: 80%;
  background: #fff;
  height: 100%;
}

.layer .scrollBox .childrenbox .list {
  padding: 0 16px;
  line-height: 40px;
  font-size: 14px;
  color: #888;
  border-bottom: 1px solid #eee;
}

.layer .active {
  border: 1px solid #ffb12a;
  color: #ffb12a;
  background: none;
}

.layer .season {
  width: 45%;
  text-align: center;
  background: #f1f2f6;
  color: #1a1b1f;
  font-size: 12px;
  margin-bottom: 10px;
  padding-top: 5px;
  padding-bottom: 5px;
  border-radius: 5px;
}

/* 底部固定 */

.footer {
  position: fixed;
  bottom: 0;
  left: 20%;
  right: 0;
  background: #fff;
  border-top: 1px solid #eee;
  box-sizing: border-box;
}

.footer .footerBtnBox {
  display: flex;
  color: #fff;
  font-size: 13px;
  box-sizing: border-box;
  width: 100%;
}

.footer .footerBtnBox .cartBtn {
  height: 40px;
  line-height: 40px;
  width: 50%;
  text-align: center;
  background: #faaf3b;
  /* border-radius: 20px 0 0 20px; */
}

.footer .footerBtnBox .buyBtn {
  height: 40px;
  line-height: 40px;
  width: 50%;
  text-align: center;
  background: rgb(240, 90, 36);
  /* border-radius: 0 20px 20px 0; */
}

.footer .service {
  flex: 1;
  padding-right: 16px;
  display: flex;
  align-items: center;
}

.footer .service .iconList {
  color: #999;
  font-size: 12px;
}

.footer .service .iconList:first-child {
  margin-right: 40px;
}

.footer .service .iconList .icon {
  width: 20px;
  height: 20px;
  margin: 0 auto 4px auto;
}

/* 搜索框样式 */

.weui-search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  box-sizing: border-box;
}

.weui-icon-search {
  margin-right: 8px;
  font-size: inherit;
}

.weui-icon-search_in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}

.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 5px;
  background: #fff;
  border: 1rpx solid #e6e6ea;
}

.weui-search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.weui-search-bar__input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}

.weui-icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 8px;
  font-size: 0;
}

.weui-search-bar__label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  color: #9b9b9b;
  background: #fff;
  line-height: 28px;
}

.weui-search-bar__cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #ffb12a;
  white-space: nowrap;
}
