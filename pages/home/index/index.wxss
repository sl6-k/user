.homeHeader {
  position: relative;
}

.homeHeader .headerBg {
  position: absolute;
  top: 0;
  left: 0px;
  right: 0px;
  z-index: 0;
}

.homeHeader .headerBg image {
  width: 100%;
}

.homeHeader .content {
  position: relative;
  z-index: 2;
}

.homeHeader .headerTitle {
  padding: 0 15px;
  display: flex;
  position: relative;
  align-items: center;
}

.homeHeader .headerTitle .localtion {
  display: flex;
  position: absolute;
  height: 100%;
  left: 15px;
  align-items: center;
  color: #fff;
  font-size: 13px;
}

.homeHeader .headerTitle .search {
  display: flex;
  position: absolute;
  height: 100%;
  left: 25%;
  align-items: center;
  font-size: 12px;
}

.searchInput {
  background: #fff;
  border-radius: 20px;
  overflow: hidden;
  padding: 0 15px;
  box-sizing: border-box;
  display: flex;
  flex: 1;
  align-items: center;
}

.searchInput  input {
  flex: 1;
  padding: 0 10px;
  width: 350rpx;
  font-size: 12px;
}

.homeHeader .sites {
  max-width: 40px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.homeHeader .localtionIcon {
  width: 12px;
  height: 12px;
  margin-right: 5px;
}

.homeHeader .localtionB {
  width: 10px;
  height: 10px;
  margin-left: 5px;
}

.homeHeader .title {
  width: 120px;
  margin: 0 auto;
}

.homeHeader .title image {
  width: 100%;
}

.homeHeader .cartSetting {
  width: 90%;
  margin: 5px auto;
  background: #fff;
  padding: 10px 16px;
  border-radius: 10px;
  box-sizing: border-box;
  position: relative;
  z-index: 2;
}

.homeHeader .cartSetting .noCart {
  text-align: center;
  color: #333;
  font-size: 16px;
  /* padding: 20px 0; */
}

.homeHeader .cartSetting .noCart .iconBox {
  width: 60px;
  margin: 0 auto;
}

.homeHeader .cartSetting .noCart .iconBox image {
  margin-left: 10px;
  width: 100%;
}

.homeHeader .cartSetting .cart .select {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #4d4d4d;
  font-size: 16px;
}

.homeHeader .cartSetting .cart .select navigator {
  color: #666;
  font-size: 14px;
}

.homeHeader .cartSetting .cart  .cartCentent {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-top: 10px;
}

.homeHeader .cartSetting .cart  .cartCentent .cartImg {
  width: 40px;
  margin-right: 20px;
}

.homeHeader .cartSetting .cart  .cartCentent .cartImg image {
  width: 40px;
  height: 40px;
}

.homeHeader .cartSetting .cart  .cartCentent .cartText {
  flex: 1;
  font-size: 16px;
  color: #1a1a1a;
  line-height: 30px;
  height: 60px;
}

.homeHeader .cartSetting .cart  .cartCentent .cartText .text {
  font-size: 14px;
  color: #808080;
}

