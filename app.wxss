}

/*主题色*/

.themeColor {
  color: #ffb12a !important;
}

.themeBgcolor {
  background-color: #ffb12a !important;
}

/*隐藏滚动条*/

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/**
*	fixed
*/

.fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
}

.fixed .back {
  width: 24px;
  height: 24px;
  margin-left: 20px;
}

/**flex-box**/

.flexCenter {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

/**image**/

.images {
  display: block;
  width: 100%;
  height: 100%;
}

page {
  width: 100%;
  height: 100%;
  background: #f2f2f2;
}

.container {
  width: 100%;
  height: 100%;
  color: #333;
  font-size: 14px;
  position: relative;
}

.textRight {
  text-align: right !important;
}

.textLeft {
  text-align: left !important;
}

.fontSize14 {
  font-size: 14px;
  color: #666;
}

/******list 右箭头*******/

.blockList {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 14px 16px;
  border-bottom: 1px solid #eee;
  background: #fff;
  color: #333;
  font-size: 16px;
}

.blockList:active {
  background: #efefef;
}

.blockList .listIcon {
  width: 22px;
  height: 22px;
  overflow: hidden;
}

.blockList .listIcon image {
  width: 100%;
  height: 100%;
}

.blockList .listText {
  flex: 1;
  padding-left: 15px;
  font-size: 15px;
}

.blockList .listText .msg {
  color: #ccc;
  font-size: 12px;
  margin-top: 4px;
}

.blockList .listText .title {
  font-size: 16px;
}

.blockList .listRightIcon {
  width: 12px;
  height: 12px;
  overflow: hidden;
}

.blockList .listRightIcon image {
  display: block;
  width: 100%;
  height: 100%;
}

.btnBox {
  padding: 30px 16px;
}

.btnBox button {
  background: #ffb12a;
  border-radius: 24px;
}

.btnBox button.button-hover {
  background: #ffb12a;
}

.btnBox button::after {
  border: none;
}

.wxBtn button {
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}

/**** list input输入****/

.listInputBox {
  padding: 10px 16px;
  font-size: 16px;
  border-top: 1px solid #eee;
  background: #fff;
}

.listInputBox .listImage {
  flex: 1;
  text-align: right;
  overflow: hidden;
}

.listInputBox .listImage .photoBox {
  width: 24px;
  height: 24px;
  float: right;
  overflow: hidden;
}

.listInputBox .listImage .photoBox image {
  width: 100%;
  height: 100%;
}

.listInputBox .listInput {
  flex: 1;
  padding-left: 20px;
}

.listInputBox .listInput input {
  width: 100%;
  text-align: right;
  font-size: 14px;
  color: #666;
  line-height: 26px;
  height: 26px;
}

/**
暂无数据
*/

.none {
  min-width: 180px;
  color: #ccc;
  font-size: 14px;
  text-align: center;
  padding: 50px 0;
}

.none .icon {
  width: 120px;
  margin: 0 auto;
}

.none .icon image {
  display: block;
  width: 100%;
}

.loadingMsg {
  min-width: 180px;
  text-align: center;
  font-size: 14px;
  color: #ccc;
  padding: 20px 0;
}
